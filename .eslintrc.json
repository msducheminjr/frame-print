{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "script"
    },
    "rules": {
        "no-console": "off"
    },
    "overrides": [
        {
          "files": [
                "**/*.test.js"
          ],
          "env": {
                "jest": true
          }
        }
    ]
}
